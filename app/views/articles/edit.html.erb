<h2> Editing Article: <%= @article.title %></h2>

<p> <%= @article.content %> </p>

<%= form_with model: @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>

<%= link_to "Return to Index", articles_path %>
